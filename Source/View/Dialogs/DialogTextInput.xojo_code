#tag WindowCode
	#tag Event
		Sub Shown()
		  SetTextStyle
		  FocusTextBox
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h21
		Private Sub AddTextMacroContextMenu(tb as WebTextArea)
		  Var menu As New WebMenuItem
		  
		  For Each s As String In session.SvcAssets.TextMacros
		    menu.AddMenuItem(s)
		  Next
		  
		  tb.ContextualMenu = menu
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub FocusTextBox()
		  If TabPanel1.SelectedPanelIndex = 0 Then
		    tbLeft.SetFocus
		  Else
		    tbRight.SetFocus
		  End
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub SetControlsDefaults()
		  If TabPanel1.SelectedPanelIndex = 0 Then
		    If Self.mTextLeft <> Nil Then
		      Select Case Self.mTextLeft.Alignment
		      Case Integer(TextAlignments.Left)
		        SegButtonAlignment.SelectedSegmentIndex = 0
		      Case Integer(TextAlignments.Center)
		        SegButtonAlignment.SelectedSegmentIndex = 1
		      Case Integer(TextAlignments.Right)
		        SegButtonAlignment.SelectedSegmentIndex = 2
		      End
		    End
		  Else
		    If Self.mTextRight <> Nil Then
		      Select Case Self.mTextRight.Alignment
		      Case Integer(TextAlignments.Left)
		        SegButtonAlignment.SelectedSegmentIndex = 0
		      Case Integer(TextAlignments.Center)
		        SegButtonAlignment.SelectedSegmentIndex = 1
		      Case Integer(TextAlignments.Right)
		        SegButtonAlignment.SelectedSegmentIndex = 2
		      End
		    End
		  End
		  
		  If TabPanel1.SelectedPanelIndex = 0 Then
		    If Self.mTextLeft <> Nil Then
		      fontSize.SelectRowWithText(Str(Self.mTextLeft.FontSize))
		    End
		  Else
		    If Self.mTextRight<> Nil Then
		      fontSize.SelectRowWithText(Str(Self.mTextRight.FontSize))
		    End
		  End
		  
		  If TabPanel1.SelectedPanelIndex = 0 Then
		    If Self.mTextLeft <> Nil And fontNames.HasValue(Self.mTextLeft.FontName) Then
		      fontNames.SelectRowWithValue(Self.mTextLeft.FontName)
		    Else
		      fontNames.SelectedRowIndex = 0
		    End
		    Self.mTextLeft.FontName = fontNames.SelectedRowValue
		  Else
		    If Self.mTextRight <> Nil And fontNames.HasValue(Self.mTextRight.FontName) Then
		      fontNames.SelectRowWithValue(Self.mTextRight.FontName)
		    Else
		      fontNames.SelectedRowIndex = 0
		    End
		    Self.mTextRight.FontName = fontNames.SelectedRowValue
		  End
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub SetElem(left as RibbonElementText, right as RibbonElementText)
		  Self.mTextLeft = Left
		  Self.mTextRight = Right
		  
		  Self.tbLeft.Text = Left.Text
		  Self.tbRight.Text = Right.Text
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub SetTextStyle()
		  Var style As New WebStyle
		  style.Italic = Self.mTextLeft.IsItalic
		  style.Bold = Self.mTextLeft.IsBold
		  
		  tbLeft.Style = style
		  tbLeft.TextAlignment = Xojo.TextAlignments(mTextLeft.Alignment)
		  tbLeft.UpdateBrowser
		  
		  tbRight.Style = style
		  tbRight.TextAlignment = Xojo.TextAlignments(mTextRight.Alignment)
		  tbRight.UpdateBrowser
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub Show(side as string = "left")
		  If Self.TabPanel1.PanelCount > 1 And Session.CurrentWorkload.SingleRibbon Then
		    Self.TabPanel1.RemovePanelAt(1)
		    Self.TabPanel1.CaptionAt(0) = "Schleifentext"
		  End
		  
		  If side = "left" Then
		    Self.TabPanel1.SelectedPanelIndex = 0
		  Else
		    Self.TabPanel1.SelectedPanelIndex = 1
		  End
		  
		  // Calling the overridden superclass method.
		  Super.Show()
		  
		End Sub
	#tag EndMethod


	#tag Hook, Flags = &h0
		Event OkClicked(left as RibbonElementText, right as RibbonElementText)
	#tag EndHook


