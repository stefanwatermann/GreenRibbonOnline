#tag Events fontNames
	#tag Event
		Sub SelectionChanged(item As WebMenuItem)
		  If TabPanel1.SelectedPanelIndex = 0 Then
		    If Self.mTextLeft <> Nil Then
		      Self.mTextLeft.FontName = Me.SelectedRowValue
		    End
		  Else
		    If Self.mTextRight <> Nil Then
		      Self.mTextRight.FontName = Me.SelectedRowValue
		    End
		  End
		  
		  SetTextStyle
		End Sub
	#tag EndEvent
	#tag Event
		Sub Opening()
		  Me.RemoveAllRows
		  
		  For Each f As FontDef In Session.SvcFonts.Fonts
		    Me.AddRow(f.DisplayName)
		  Next
		End Sub
	#tag EndEvent
	#tag Event
		Sub Shown()
		  SetControlsDefaults
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events btnOk
	#tag Event
		Sub Pressed()
		  Self.Close
		  RaiseEvent OkClicked(self.mTextLeft, self.mTextRight)
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events btnCancel
	#tag Event
		Sub Pressed()
		  Self.close
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events SegButtonAlignment
	#tag Event
		Sub Pressed(segmentIndex As Integer)
		  Select Case segmentIndex
		    
		  Case 0
		    If TabPanel1.SelectedPanelIndex = 0 Then
		      Self.mTextLeft.Alignment = Integer(TextAlignments.Left)
		    Else
		      Self.mTextRight.Alignment = Integer(TextAlignments.Left)
		    End
		    
		  Case 1
		    If TabPanel1.SelectedPanelIndex = 0 Then
		      Self.mTextLeft.Alignment = Integer(TextAlignments.Center)
		    Else
		      Self.mTextRight.Alignment = Integer(TextAlignments.Center)
		    End
		    
		  Case 2
		    If TabPanel1.SelectedPanelIndex = 0 Then
		      Self.mTextLeft.Alignment = Integer(TextAlignments.Right)
		    Else
		      Self.mTextRight.Alignment = Integer(TextAlignments.Right)
		    end
		    
		  End
		  
		  SetTextStyle
		End Sub
	#tag EndEvent
	#tag Event
		Sub Shown()
		  SetControlsDefaults()
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events fontSize
	#tag Event
		Sub SelectionChanged(item As WebMenuItem)
		  If TabPanel1.SelectedPanelIndex = 0 Then
		    If Self.mTextLeft <> Nil Then
		      Self.mTextLeft.FontSize = Val(Me.SelectedRowValue)
		    End
		  Else
		    If Self.mTextRight<> Nil Then
		      Self.mTextRight.FontSize = Val(Me.SelectedRowValue)
		    End
		  End
		  
		  SetTextStyle
		End Sub
	#tag EndEvent
	#tag Event
		Sub Opening()
		  Me.RemoveAllRows
		  
		  For Each size As Integer In Session.SvcFonts.FontSizes
		    Me.AddRow(Str(size))
		  Next
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Shown()
		  SetControlsDefaults
		End Sub
