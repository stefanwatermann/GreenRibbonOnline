#tag WindowCode
	#tag Event
		Sub Resized()
		  ArrangeContorls
		  RenderRibbons
		End Sub
	#tag EndEvent

	#tag Event
		Sub Shown()
		  ArrangeContorls
		  
		  Var r As Ribbon = Session.CurrentWorkload.LeftRibbon
		  
		  // neues Design bei "new" oder wenn das Design nicht initialisiert ist
		  If Session.CurrentAction = "new" Or r = Nil Or Not r.Design.IsInitialized Then
		    VerticalRibbonEditButtons1.OrderButtonVisible(True)
		    CreateNewDesign
		    
		  ElseIf Session.CurrentAction = "modify" Then
		    VerticalRibbonEditButtons1.OrderButtonVisible(False)
		    
		  End
		  
		  RenderRibbons
		  
		  Session.CurrentAction = ""
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h21
		Private Sub ArrangeContorls()
		  Var center As Integer = (Self.Width / 2)
		  Var btnWidth As Integer = Self.VerticalRibbonEditButtons1.Width
		  Var ribbonWidth As Integer = center - (btnWidth / 2)
		  
		  Self.RibbonLeftContainer.Left = 0
		  Self.RibbonLeftContainer.Width =  ribbonWidth
		  
		  Self.RibbonRightContainer.Left = ribbonWidth + btnWidth
		  Self.RibbonRightContainer.Width =  ribbonWidth
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub CreateNewDesign()
		  // create new ribbon instances
		  Session.CurrentWorkload.LeftRibbon = New Ribbon
		  Session.CurrentWorkload.RightRibbon = New Ribbon
		  
		  // init default design
		  Var design As New RibbonDesign
		  design.Dimension = DimensionDef(Session.SvcAssets.DimensionDefs.ValueAt(0))
		  design.Material = MaterialDef(Session.SvcAssets.Materials.ValueAt(0))
		  design.MaterialEnding = MaterialEndingDef(Session.SvcAssets.MaterialEndings.ValueAt(0))
		  design.PrintColor = ColorDef(Session.SvcAssets.ColorDefs.ValueAt(0))
		  
		  // assign default design
		  Session.CurrentWorkload.LeftRibbon.Design = design
		  Session.CurrentWorkload.RightRibbon.Design = design
		  
		  ShowDesignDialog
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub GraphicSelected(sender as DialogGraphicInput, leftPicture as Picture, leftFilename as string, rightPicture as Picture, rightFilename as string, leftStretchFactor as Double, rightStretchFactor as double)
		  Session.CurrentWorkload.LastSaved = DateTime.Now
		  
		  Session.CurrentWorkload.LeftRibbon.GraphicElement.Image = leftPicture
		  Session.CurrentWorkload.LeftRibbon.GraphicElement.Filename = leftFilename
		  Session.CurrentWorkload.LeftRibbon.GraphicElement.StretchFactor = leftStretchFactor
		  
		  Session.CurrentWorkload.RightRibbon.GraphicElement.Image = rightPicture
		  Session.CurrentWorkload.RightRibbon.GraphicElement.Filename = rightFilename
		  Session.CurrentWorkload.RightRibbon.GraphicElement.StretchFactor = rightStretchFactor
		  
		  Session.ResetExecutionTime
		  
		  RenderRibbons
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub RenderRibbons()
		  VerticalRibbonEditButtons1.ShowLoadingIndicator(True)
		  
		  RibbonLeftContainer.Render(Session.CurrentWorkload.LeftRibbon)
		  RibbonRightContainer.Render(Session.CurrentWorkload.RightRibbon)
		  
		  VerticalRibbonEditButtons1.ShowRibbonSize(_
		  Session.CurrentWorkload.LeftRibbon.Design.mmWidth, _
		  Session.CurrentWorkload.LeftRibbon.Design.mmHeight)
		  
		  VerticalRibbonEditButtons1.ShowLoadingIndicator(False)
		  
		  FooterContainer1.UpdateExecutionTime
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub SetExecutionTime()
		  FooterContainer1.UpdateExecutionTime
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub ShowAddToCartDialog()
		  If Session.CurrentWorkload.LeftRibbon.HasElements Or Session.CurrentWorkload.RightRibbon.HasElements Then
		    
		    Var item As New CartItem
		    item.LeftRibbon = Session.CurrentWorkload.LeftRibbon
		    item.RightRibbon = Session.CurrentWorkload.RightRibbon
		    
		    DialogRibbonAddToCart1.Show(item)
		    
		  Else
		    MessageBox("Bitte geben Sie auf einer der beiden Seiten einen Text ein oder w√§hlen Sie eine Grafik, bevor Sie die Schleifen in den Warenkorb legen.")
		    
		  End
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub ShowDesignDialog()
		  DialogRibbonDesign1.Show(session.CurrentWorkload.LeftRibbon.Design)
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub ShowGraphicDialog(side as string)
		  Var dlg As New DialogGraphicInput
		  dlg.SetElem(Session.CurrentWorkload.LeftRibbon.GraphicElement, Session.CurrentWorkload.RightRibbon.GraphicElement)
		  AddHandler dlg.OkClicked, AddressOf GraphicSelected
		  dlg.Show(side)
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub ShowLayoutDialog()
		  DialogRibbonLayout1.SetLayout(Session.CurrentWorkload.LeftRibbon.Layout, Session.CurrentWorkload.RightRibbon.Layout)
		  DialogRibbonLayout1.Show
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub ShowTextDialog(side as string)
		  TextInputDialog1.SetElem(Session.CurrentWorkload.LeftRibbon.TextElement, Session.CurrentWorkload.RightRibbon.TextElement)
		  TextInputDialog1.Show(side)
		End Sub
	#tag EndMethod


#tag EndWindowCode

#tag Events DialogRibbonDesign1
	#tag Event
		Sub OkClicked(direction as integer, material as MaterialDef, materialEnding as MaterialEndingDef, printColor as ColorDef, dimension as DimensionDef)
		  Var design As New RibbonDesign
		  design.mmHeight = dimension.MmHeight
		  design.mmWidth = dimension.MmWidth
		  design.Material = material
		  design.MaterialEnding = materialEnding
		  design.PrintColor = printColor
		  design.Dimension = dimension
		  design.IsHorizontal = direction = 0
		  
		  session.CurrentWorkload.LeftRibbon.Design = design
		  session.CurrentWorkload.RightRibbon.Design = design
		  
		  If design.IsHorizontal Then
		    session.CurrentWorkload.LeftRibbon.Layout.BottomDistance = 20
		    session.CurrentWorkload.RightRibbon.Layout.BottomDistance = 20
		  Else
		    session.CurrentWorkload.LeftRibbon.Layout.BottomDistance = 0
		    session.CurrentWorkload.RightRibbon.Layout.BottomDistance = 0
		  End
		  
		  ArrangeContorls
		  
		  RenderRibbons
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events DialogRibbonLayout1
	#tag Event
		Sub OkClicked(left as RibbonLayout, right as RibbonLayout)
		  Session.ResetExecutionTime
		  
		  RenderRibbons
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events RibbonLeftContainer
	#tag Event
		Sub Pressed(elem as RibbonElementBase, x as integer, y as integer)
		  If elem = Nil Then
		    ShowTextDialog("left")
		    
		  Else
		    If elem IsA RibbonElementText Then
		      ShowTextDialog("left")
		      
		    ElseIf elem IsA RibbonElementGraphic Then
		      ShowGraphicDialog("left")
		      
		    End
		  End
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events RibbonRightContainer
	#tag Event
		Sub Pressed(elem as RibbonElementBase, x as integer, y as integer)
		  If elem = Nil Then
		    ShowTextDialog("right")
		    
		  Else
		    If elem IsA RibbonElementText Then
		      ShowTextDialog("right")
		      
		    ElseIf elem IsA RibbonElementGraphic Then
		      ShowGraphicDialog("right")
		      
		    End
		  End
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events DialogRibbonAddToCart1
	#tag Event
		Sub OkClicked(item as CartItem)
		  Session.CurrentWorkload.Cart.Add(item)
		  Session.CurrentAction = "new"
		  GoToURL("/")
		End Sub
	#tag EndEvent
	#tag Event
		Sub OkCartClicked(item as CartItem)
		  Session.CurrentWorkload.Cart.Add(item)
		  GoToURL("#cart")
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events VerticalRibbonEditButtons1
	#tag Event
		Sub ButtonPressed(name as string)
		  Select Case name
		    
		  Case "btnDesign"
		    ShowDesignDialog()
		    
		  Case "btnText"
		    ShowTextDialog("left")
		    
		  Case "btnGraphic"
		    ShowGraphicDialog("left")
		    
		  Case "btnLayout"
		    ShowLayoutDialog()
		    
		  Case "btnOrder"
		    ShowAddToCartDialog()
		    
		  Case "btnShowCart"
		    GoToURL("#cart")
		    
		  Case "btnNew"
		    CreateNewDesign()
		    
		  End
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events TextInputDialog1
	#tag Event
		Sub OkClicked(left as RibbonElementText, right as RibbonElementText)
		  Session.ResetExecutionTime
		  
		  Session.CurrentWorkload.LastSaved = DateTime.Now
		  Session.CurrentWorkload.LeftRibbon.TextElement = Left
		  Session.CurrentWorkload.RightRibbon.TextElement = Right
		  
		  RenderRibbons
		  
		End Sub
	#tag EndEvent
#tag EndEvents
