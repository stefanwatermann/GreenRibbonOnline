#tag WindowCode
	#tag Event
		Sub Resized()
		  ArrangeContorls
		  RenderRibbons
		End Sub
	#tag EndEvent

	#tag Event
		Sub Shown()
		  Init
		  
		  ArrangeContorls
		  
		  Var r As Ribbon = Session.CurrentWorkload.LeftRibbon
		  
		  // neues Design bei "new" oder wenn das Design nicht initialisiert ist
		  If Session.CurrentAction = "new" Or r = Nil Or Not r.Design.IsInitialized Then
		    VerticalRibbonEditButtons1.OrderButtonVisible(True)
		    CreateNewDesign
		    
		  ElseIf Session.CurrentAction = "modify" Then
		    VerticalRibbonEditButtons1.OrderButtonVisible(False)
		    
		  End
		  
		  RenderRibbons
		  
		  Session.CurrentAction = ""
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h21
		Private Sub ArrangeContorls()
		  //Var center As Integer = (Self.Width / 2)
		  //Var btnWidth As Integer = Self.VerticalRibbonEditButtons1.Width
		  //Var ribbonWidth As Integer = center - (btnWidth / 2)
		  //
