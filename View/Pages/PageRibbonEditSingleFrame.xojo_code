#tag Events VerticalRibbonEditButtons1
	#tag Event
		Sub ButtonPressed(name as string)
		  Select Case name
		    
		  Case "btnDesign"
		    ShowDesignDialog()
		    
		  Case "btnText"
		    ShowTextDialog()
		    
		  Case "btnGraphic"
		    ShowGraphicDialog()
		    
		  Case "btnLayout"
		    ShowLayoutDialog()
		    
		  Case "btnOrder"
		    If Session.CurrentWorkload.UsesExternalCart Then
		      MessageBox("Weiterleitung zum Warenkorb (Übergabe der Schleifendaten individuell abzustimmen, z.B. per URL oder REST)...")
		    Else
		      ShowAddToCartDialog()
		    End
		    
		  Case "btnShowCart"
		    If Session.CurrentWorkload.UsesExternalCart Then
		      MessageBox("Weiterleitung zum Warenkorb (Übergabe der Schleifendaten individuell abzustimmen, z.B. per URL oder REST)...")
		    Else
		      GoToURL("#cart")
		    End
		    
		  Case "btnNew"
		    CreateNewDesign()
		    
		  End
		End Sub
	#tag EndEvent
#tag EndEvents
